<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" " http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="loginAction" class="com.joyque.action.LoginAction" scope="prototype">
		<property name="loginService" ref="loginService"></property>		
	</bean>
	
	<bean id="userBasicAction" class="com.joyque.action.UserBasicAction" scope="prototype">
		<property name="userBasicService" ref="userBasicService"></property>
	</bean>
	
	<bean id="activityAction" class="com.joyque.action.ActivityAction" scope="prototype">
		<property name="activityService" ref="activityService"></property>
	</bean>
	
	<bean id="questionAction" class="com.joyque.action.QuestionAction" scope="prototype">
		<property name="questionService" ref="questionService"></property>
	</bean>
	
	<bean id="uplordAction" class="com.joyque.action.UplordAction" scope="prototype">
		<property name="uplordService" ref="uplordService"></property>
	</bean>
</beans>